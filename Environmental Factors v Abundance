    geom_point()+ ggtitle("Velocity of Water Current 1996-2012 by Field Station")+
    theme(plot.title = element_text(lineheight=.8, face="bold",size = 22)) + 
    theme(axis.title.x=element_text(size=16),axis.title.y=element_text(size=16)) +
    xlab("Velocity (m/s)")+ ylab("Biovolume of Cyanobacteria (m^3/L)")+
    theme_fivethirtyeight()+ theme(axis.title=element_text()) + facet_wrap(~FLDNUM,ncol=3)
  phyto_cfldV
#________________________________________________________________________  
#Total Temp over time
   phyto_cTp<-ggplot(phyto_cynTVT,aes(TEMP,BIOVOLUME))+
    geom_point()+ ggtitle("Water Temperature 1996-2012 Overall")+
    theme(plot.title = element_text(lineheight=.8, face="bold",size = 22)) + 
    theme(axis.title.x=element_text(size=16),axis.title.y=element_text(size=16)) +
    xlab("Temperature (C)")+ ylab("Biovolume of Cyanobacteria (m^3/L)")+
    theme_fivethirtyeight()+ theme(axis.title=element_text()) 
  phyto_cTp
#Separate by FieldNum
  phyto_cfldTp<-ggplot(phyto_cynTVT,aes(TEMP,BIOVOLUME))+
    geom_point()+ ggtitle("Water Temperature 1996-2012 by Field Station")+
    theme(plot.title = element_text(lineheight=.8, face="bold",size = 22)) + 
    theme(axis.title.x=element_text(size=16),axis.title.y=element_text(size=16)) +
    xlab("Temperature (C)")+ ylab("Biovolume of Cyanobacteria (m^3/L)")+
    theme_fivethirtyeight()+ theme(axis.title=element_text()) + facet_wrap(~FLDNUM,ncol=3)
  phyto_cfldTp
  
